# Instagram Sync Settings
REACT_APP_INSTAGRAM_SYNC_INTERVAL=86400000
REACT_APP_INSTAGRAM_DEFAULT_SYNC_TIME=09:00
REACT_APP_INSTAGRAM_MAX_POSTS_FETCH=25

# Feature Flags
REACT_APP_ENABLE_INSTAGRAM_INSIGHTS=true
REACT_APP_ENABLE_INSTAGRAM_AUTO_SYNC=true
